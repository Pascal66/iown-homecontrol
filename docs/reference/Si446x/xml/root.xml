          <description activation_logic="" visibility_logic="">
            <text>Disable pending interrupts in the Chip Status group from asserting nNIRQ.</text>
          </description>
        </descriptions><comments/></enum><enum name="ENABLED" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Enable pending interrupts in the Chip Status group to assert nNIRQ.</text>
          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="LOGIC" id="5fbffd4b-62d6-11e4-ae6c-180373e564d0"><enum name="enum_0" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Logical result from this Match byte is AND'ed with the logical result from previous MATCH field(s).</text>
          </description>
        </descriptions><comments/></enum><enum name="enum_1" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Logical result from this Match byte is OR'ed with the logical result from previous MATCH field(s).</text>
          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="MANCHESTER" id="5fe93135-62d6-11e4-8b7e-180373e564d0"><enum name="enum_0" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Disable Manchester decoding on this field.</text>
          </description>
        </descriptions><comments/></enum><enum name="enum_1" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Enable Manchester decoding on this field.</text>
          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="MOD_SOURCE" id="5f2c6c0a-62d6-11e4-a498-180373e564d0"><enum name="PACKET" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>The modulation is sourced from the TX FIFO in the packet handler.</text>
          </description>
        </descriptions><comments/></enum><enum name="DIRECT" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>The modulation is sourced in real-time (i.e., TX Direct Mode) from a GPIO pin, as selected by the TX_DIRECT_MODE_GPIO field.</text>
          </description>
        </descriptions><comments/></enum><enum name="PSEUDO" short_desc="" value="2" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>The modulation is sourced from the internal pseudo-random generator.</text>
          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="MODE" id="60324a0c-62d6-11e4-8580-180373e564d0"><enum name="busy" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Channel is busy</text>
          </description>
        </descriptions><comments/></enum><enum name="clear" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Channel is clear</text>
          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="MODEM_INT_STATUS_EN" id="5f2cba2d-62d6-11e4-b885-180373e564d0"><enum name="DISABLED" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Disable pending interrupts in rx mode group from asserting nNIRQ.</text>
          </description>
