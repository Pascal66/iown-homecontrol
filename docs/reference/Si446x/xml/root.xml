      <property name="MATCH_MASK_1" group_name="MATCH" item_name="MASK_1" short_desc="" group_num="0x30" index_start="0x01" index_end="0x01" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="((mode_PRO2 || mode_EZR2) &amp;&amp; feature_MATCH_GRP)">
