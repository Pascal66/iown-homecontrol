              <changelog/>
              <descriptions>
        <description activation_logic="" visibility_logic="">
          <text>Digital power sequencing delay between PA steps defined in .1us.</text>
        </description>
      </descriptions>
              <comments/>
              <parameter_set_mapping>
                <mapping parameter_start_index="0x06" parameter_bit_msb="6" fsize="7"/>
              </parameter_set_mapping>
            </field>
          </parameter_set>
        </property_activation>
      </property>
