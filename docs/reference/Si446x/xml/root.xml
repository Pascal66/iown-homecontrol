      <property name="MODEM_OOK_PDTC" group_name="MODEM" item_name="OOK_PDTC" short_desc="" group_num="0x20" index_start="0x40" index_end="0x40" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="feature_MODEM_GRP &amp;&amp; (mode_PRO2 || mode_IE154)">
