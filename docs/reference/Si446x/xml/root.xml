          </description>
        </descriptions><comments/></enum></enum_set>
    <enum_set name="IE154_STATUS" id="5f27ff1e-62d6-11e4-9a69-180373e564d0"><enum name="status_success" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>The operation completed successfully.</text>
          </description>
        </descriptions><comments/></enum><enum name="status_ack_timing_invalid" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
