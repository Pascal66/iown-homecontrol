            <field name="SW_CRC_CTRL" short_desc="" fsize="1" ftype="ENUM" fformat="hex" value_min="" value_max="" value_default="0" unit="" scale="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="">
