              </parameter_set_mapping>
              <enum_set_id>5fd0033b-62d6-11e4-8091-180373e564d0</enum_set_id>
            </field>
            <field name="ENRSSIJMP" short_desc="" fsize="1" ftype="ENUM" fformat="hex" value_min="" value_max="" value_default="0" unit="" scale="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="">
