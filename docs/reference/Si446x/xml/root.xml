            <field name="ADC_GPIO_EN" short_desc="" fsize="1" ftype="ENUM" fformat="hex" value_min="" value_max="" value_default="" unit="" scale="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="">
              <changelog/>
              <descriptions/>
              <comments/>
              <parameter_set_mapping>
