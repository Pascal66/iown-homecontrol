      <command name="AGC_OVERRIDE" short_desc="" number="0xd0" cmd_type="args_cts_data_reply" visibility_logic="access_FACTORY" status="complete" status_detail="" release_detail="" activation_logic="(mode_PRO2 || mode_IE154 || mode_EZR2)">
