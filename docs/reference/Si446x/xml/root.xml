                <mapping parameter_start_index="0x06" parameter_bit_msb="1" fsize="2"/>
              </parameter_set_mapping>
              <enum_set_id>5ffc4482-62d6-11e4-a3ea-180373e564d0</enum_set_id>
            </field>
          </parameter_set>
        </property_activation>
      </property>
