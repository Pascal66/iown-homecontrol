                <mapping parameter_start_index="0x04" parameter_bit_msb="7" fsize="8" split_msb="15" split_lsb="8"/>
                <mapping parameter_start_index="0x05" parameter_bit_msb="7" fsize="8" split_msb="7" split_lsb="0"/>
              </parameter_set_mapping>
            </field>
          </parameter_set>
        </inputs>
        <outputs>
