        </descriptions><comments/></enum></enum_set>
    <enum_set name="IE154_INT_CTL_CHIP_INT_STATUS_EN" id="6000ff8e-62d6-11e4-b8eb-180373e564d0"><enum name="DISABLED" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail=""><changelog/><descriptions>
