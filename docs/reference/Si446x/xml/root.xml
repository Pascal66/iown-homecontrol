    <enum_set name="LENGTH_ENUM" id="5ff873db-62d6-11e4-87d8-180373e564d0"><enum name="LEN_1_BYTES" short_desc="" value="0" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Sync Word is 1-byte (8 bits) in length.  Sync Word byte 3 [31:24] is used, with its value defined in the </text>
            <reference ref_type="ref_prop" ref_text="SYNC_BITS"/>
            <text> property.</text>
          </description>
        </descriptions><comments/></enum><enum name="LEN_2_BYTES" short_desc="" value="1" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Sync Word is 2-bytes (16 bits) in length.  Sync Word bytes 3 and 2 [31:16] are used (in descending order), with their values defined in the </text>
            <reference ref_type="ref_prop" ref_text="SYNC_BITS"/>
            <text> property.</text>
          </description>
        </descriptions><comments/></enum><enum name="LEN_3_BYTES" short_desc="" value="2" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Sync Word is 3-bytes (24 bits) in length.  Sync Word bytes 3, 2, and 1 [31:8] are used (in descending order), with their values defined in the </text>
            <reference ref_type="ref_prop" ref_text="SYNC_BITS"/>
            <text> property.</text>
          </description>
        </descriptions><comments/></enum><enum name="LEN_4_BYTES" short_desc="" value="3" activation_logic="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="revB1A"><changelog/><descriptions>
          <description activation_logic="" visibility_logic="">
            <text>Sync Word is 4-bytes (32 bits) in length.  Sync Word bytes 3, 2, 1, and 0 [31:0] are used (in descending order), with their values defined in the </text>
            <reference ref_type="ref_prop" ref_text="SYNC_BITS"/>
            <text> property.</text>
          </description>
        </descriptions><comments/></enum></enum_set>
