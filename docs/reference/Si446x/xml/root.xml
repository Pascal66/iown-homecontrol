            <field name="DSM_LSB" short_desc="" fsize="1" ftype="ENUM" fformat="hex" value_min="" value_max="" value_default="1" unit="" scale="" visibility_logic="access_PUBLIC" status="complete" status_detail="" release_detail="" activation_logic="">
