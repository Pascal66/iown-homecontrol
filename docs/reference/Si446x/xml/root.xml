              <descriptions>
        <description activation_logic="" visibility_logic="">
          <text>Selects whether the Moving Average or Min-Max slicing threshold search engine is frozen upon gear switching. </text>
        </description>
      </descriptions>
              <comments/>
              <parameter_set_mapping>
                <mapping parameter_start_index="0x50" parameter_bit_msb="7" fsize="1"/>
              </parameter_set_mapping>
              <enum_set_id>5f2cba2c-62d6-11e4-a520-180373e564d0</enum_set_id>
