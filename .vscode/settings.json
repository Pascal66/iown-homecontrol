{
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "github.vscode-github-actions",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": false
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "vscode.markdown-language-features",
    "editor.wordWrap": "off"
  },
  "[python]": {
    "editor.autoIndent": "full",
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.detectIndentation": false,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[yaml]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.detectIndentation": false,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "anycode.language.features": {
    "folding": true
  },
  "black-formatter.importStrategy": "fromEnvironment",
  "C_Cpp.autoAddFileAssociations": true,
  "C_Cpp.autocomplete": "default",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_path": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.codeAnalysis.clangTidy.codeAction.formatFixes": true,
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.path": "C:\\Program Files\\LLVM\\bin\\clang-tidy.exe",
  "C_Cpp.codeAnalysis.clangTidy.useBuildPath": false,
  "C_Cpp.codeFolding": "enabled",
  "C_Cpp.configurationWarnings": "enabled",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "C_Cpp.default.compileCommands": "compile_commands.json",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.mergeConfigurations": true,
  "C_Cpp.dimInactiveRegions": true,
  "C_Cpp.doxygen.generatedStyle": "/**",
  "C_Cpp.enhancedColorization": "enabled",
  "C_Cpp.files.exclude": { // see search.exclude
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "site": true
  },
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.hover": "default",
  "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
  "C_Cpp.inlayHints.parameterNames.hideLeadingUnderscores": false,
  "C_Cpp.intelliSenseEngineFallback": "enabled",
