  "prettier.bracketSameLine": true,
  "prettier.ignorePath": ".vscode/.prettierignore",
  "prettier.insertPragma": true,
  "prettier.printWidth": 0,
  "prettier.proseWrap": "never",
  "prettier.quoteProps": "consistent",
  "prettier.resolveGlobalModules": true,
  "prettyxml.settings.indentSpaceLength": 2,
  "prettyxml.settings.positionAllAttributesOnFirstLine": true,
  "problems.showCurrentInStatus": true,
  "pylint.importStrategy": "fromEnvironment",
  "pylint.lintOnChange": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.enablePytestSupport": false,
  "python.analysis.importFormat": "relative",
  "python.analysis.typeCheckingMode": "standard",
  // "python.analysis.extraPaths": ["${workspaceFolder}/.venv/Lib/site-packages"],
  "python.experiments.enabled": false,
  "python.globalModuleInstallation": true,
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyArgs": [
    "--config-file",
    "${workspaceFolder}/pyproject.toml"
  ],
  "python.linting.mypyEnabled": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "search.exclude": {
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "node_modules": true,
    "site": true
  },
  "search.useIgnoreFiles": false,
  "sortJSON.contextMenu": {
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONTypeReverse": false,
    "sortJSONValuesReverse": false
  },
