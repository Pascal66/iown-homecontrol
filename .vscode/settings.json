  "C_Cpp.vcFormat.newLine.beforeCatch": true,
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": false,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": false,
  "C_Cpp.vcFormat.wrap.preserveBlocks": "allOneLineScopes",
  "clang-format.executable": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "cmake.allowCommentsInPresetsFile": true,
  "cmake.allowUnsupportedPresetsVersions": true,
  "cmake.ctest.allowParallelJobs": true,
  "cmake.ignoreCMakeListsMissing": true,
  "cmake.options.statusBarVisibility": "compact",
  "cmake.showOptionsMovedNotification": false,
  "cSpell.autoFormatConfigFile": true,
  "cSpell.enableFiletypes": [
    "github-actions-workflow",
    "github-issues"
  ],
  "cSpell.language": "en,en-US",
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.validateDirectives": true,
  "debug.console.wordWrap": false,
  "debug.showInStatusBar": "always",
  "debug.showSubSessionsInToolBar": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.wordWrap": "off",
  "doxdocgen.generic.commandSuggestionAddPrefix": true,
  "doxdocgen.generic.useGitUserEmail": true,
  "doxdocgen.generic.useGitUserName": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "editor.quickSuggestions": {
    "comments": "inline",
    "other": "on",
    "strings": "inline"
  },
  "editor.renderWhitespace": "boundary",
  "editor.renderControlCharacters": true,
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "editor.unusualLineTerminators": "auto",
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 256,
  "editor.wrappingIndent": "deepIndent",
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.migration.2_x": "off",
  "eslint.onIgnoredFiles": "warn",
  "eslint.useESLintClass": true,
  "explorer.autoRevealExclude": { // see:files.exclude
    ".cache": true,
    ".git": true,
    ".pio": true,
    ".trunk": true,
    "site": true
  },
  "explorer.excludeGitIgnore": true,
  "files.associations": {
    "*.json": "jsonc",
    "*.plist": "xml",
    "*.strings": "xml",
    "*.stringsdict": "xml",
    "*.svd": "xml"
  },
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": { // see:explorer.autoRevealExclude
    ".cache": true,
    ".git": true,
    ".pio": true,
    ".trunk": true,
    "site": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.trimTrailingWhitespaceInRegexAndStrings": false,
  "files.watcherExclude": { // see:search.exclude
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "node_modules": true,
    "site": true
  },
  "git.alwaysSignOff": true,
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.openRepositoryInParentFolders": "never",
  "git.pullBeforeCheckout": true,
  "git.repositoryScanIgnoredFolders": [
    ".cache",
    ".devcontainer",
    ".pio",
    ".private",
    ".trunk",
    "bower_components",
    "node_modules",
    "site"
  ],
  "git.supportCancellation": true,
  "git.terminalGitEditor": true,
  "git.timeline.showUncommitted": true,
  "github-actions.org-features": true,
  "github.codespaces.defaultExtensions": [
    "GitHub.codespaces",
    "GitHub.vscode-pull-request-github",
    "platformio.platformio-ide"
  ],
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.terminalLinksHandler": "vscode",
  "githubPullRequests.useReviewMode": {
    "closed": true,
    "merged": true
  },
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "hexeditor.columnWidth": 16,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "hexeditor.showDecodedText": true,
  "idf.customExtraVars": {
    "IDF_CCACHE_ENABLE": "1"
  },
  "idf.enableCCache": true,
  "idf.enableIdfComponentManager": true,
  "idf.showOnboardingOnInit": false,
  "idf.telemetry": false,
  "isort.args": [
    "--profile",
    "black"
  ],
  "jake.autoDetect": "off",
  "luahelper.format.tab_width": 2,
  "markdown-checkboxes.enable": true,
  "markdown-mermaid.darkModeTheme": "dark",
  "markdown-mermaid.lightModeTheme": "neutral",
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableLinkify": false,
  "markdown-preview-enhanced.frontMatterRenderingOption": "table",
  "markdown-preview-enhanced.hideDefaultVSCodeMarkdownPreviewButtons": false,
  "markdown-preview-enhanced.mathRenderingOption": "None",
  "markdown-preview-enhanced.mermaidTheme": "forest",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "markdown-preview-enhanced.useGitHubStylePipedLink": true,
  "markdown-preview-github-styles.colorTheme": "dark",
