  "markdown.math.enabled": false,
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preferredMdPathExtensionStyle": "includeExtension",
  "markdown.preview.linkify": false,
  "markdown.preview.typographer": true,
  "markdown.replaceSmartQuotes": true,
