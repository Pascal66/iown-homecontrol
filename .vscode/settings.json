{
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "github.vscode-github-actions",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false
  },
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "[python]": {
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.detectIndentation": false,
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2,
    "editor.wordWrap": "off"
  },
  "anycode.language.features": {
    "folding": true
  },
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.runAutomatically": true,
  "C_Cpp.default.compileCommands": "compile_commands.json",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.dimInactiveRegions": true,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": false,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": false,
  "C_Cpp.vcFormat.wrap.preserveBlocks": "allOneLineScopes",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!css",
    "!elixir",
    "!erlang",
    "!go",
    "!graphql",
    "!handlebars",
    "!haskell",
    "!html",
    "!jade",
    "!java",
    "!javascript",
    "!javascriptreact",
    "!latex",
    "!less",
    "!php",
    "!pug",
    "!rust",
    "!scala",
    "!scss",
    "!swift",
    "!typescript",
    "!typescriptreact",
    "!vue",
    "c",
    "code-text-binary",
    "cpp",
    "csharp",
    "diff",
    "git-rebase",
    "github-actions-workflow",
    "github-issues",
    "ignore",
    "ini",
    "log",
    "Log",
    "lua",
    "makefile",
    "properties",
    "search-result"
  ],
  "cSpell.import": [
    "./.vscode/cspell.json"
  ],
  "cSpell.language": "en-US",
  "debug.console.wordWrap": false,
  "debug.showInStatusBar": "always",
  "debug.showSubSessionsInToolBar": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.wordWrap": "off",
  "doxdocgen.generic.useGitUserEmail": true,
  "doxdocgen.generic.useGitUserName": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never"
  },
  "editor.comments.ignoreEmptyLines": false,
  "editor.detectIndentation": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.indentSize": "tabSize",
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "editor.links": false,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.showFoldingControls": "always",
  "editor.stickyTabStops": true,
  "editor.suggest.showStatusBar": true,
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.unfoldOnClickAfterEndOfLine": false,
  "editor.unusualLineTerminators": "auto",
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 256,
  "editor.wrappingIndent": "deepIndent",
  "explorer.autoRevealExclude": {
    "**/.cache/**": true,
    "**/.git/**": true,
    "**/.pio/**": true,
    "**/.trunk/*actions": true,
    "**/.trunk/*logs": true,
    "**/.trunk/*notifications": true,
    "**/.trunk/*out": true,
    "**/.trunk/*plugins": true,
    "**/bower_components/**": true,
    "**/node_modules/**": true,
    "**/site-packages/**": true,
    "**/site/**": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.json": "jsonc",
    "*.svd": "xml"
  },
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/.cache/**": true,
    "**/.git/**": true,
    "**/.pio/**": true,
    "**/.trunk/*actions": true,
    "**/.trunk/*logs": true,
    "**/.trunk/*notifications": true,
    "**/.trunk/*out": true,
    "**/.trunk/*plugins": true,
    "**/bower_components/**": true,
    "**/node_modules/**": true,
    "**/site-packages/**": true,
    "**/site/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.cache/**": true,
    "**/.git/**": true,
    "**/.pio/**": true,
    "**/.trunk/*actions": true,
    "**/.trunk/*logs": true,
    "**/.trunk/*notifications": true,
    "**/.trunk/*out": true,
    "**/.trunk/*plugins": true,
    "**/bower_components/**": true,
    "**/node_modules/**": true,
    "**/site-packages/**": true,
    "**/site/**": true
  },
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.alwaysSignOff": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": true,
  "git.defaultBranchName": "master",
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "never",
  "git.postCommitCommand": "sync",
  "git.pullBeforeCheckout": true,
  "git.rememberPostCommitCommand": true,
  "git.repositoryScanIgnoredFolders": [
    "**/.cache/**",
    "**/.pio/**",
    "**/.trunk/*actions",
    "**/.trunk/*logs",
    "**/.trunk/*notifications",
    "**/.trunk/*out",
    "**/.trunk/*plugins",
    "**/bower_components/**",
    "**/node_modules/**",
    "**/site-packages/**",
    "**/site/**"
  ],
  "git.showPushSuccessNotification": true,
  "git.smartCommitChanges": "tracked",
  "git.supportCancellation": true,
  "git.terminalGitEditor": true,
  "git.timeline.showUncommitted": true,
  "git.untrackedChanges": "separate",
  "git.useCommitInputAsStashMessage": true,
  "git.verboseCommit": true,
  "github.codespaces.defaultExtensions": [
    "GitHub.codespaces",
    "GitHub.vscode-pull-request-github",
    "platformio.platformio-ide"
  ],
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.focusedMode": "overview",
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.terminalLinksHandler": "vscode",
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "idf.customExtraVars": {
    "IDF_CCACHE_ENABLE": "1"
  },
  "idf.enableCCache": true,
  "idf.enableIdfComponentManager": true,
  "idf.showOnboardingOnInit": false,
  "idf.telemetry": false,
  "jake.autoDetect": "off",
  "luahelper.format.tab_width": 2,
  "markdown-preview-github-styles.colorTheme": "dark",
  "markdown.extension.math.enabled": false,
  "markdown.math.enabled": false,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.validate.enabled": true,
  "markdownlint.config": {
    "focusMode": true,
    "MD007": {
      "indent": 2
    },
    "MD012": false,
    "MD013": false,
    "MD031": false,
    "MD033": false,
    "MD041": false,
    "no-hard-tabs": true
  },
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.autoDetect": "off",
  "platformio-ide.autoCloseSerialMonitor": false,
  "platformio-ide.disablePIOHomeStartup": true,
  "problems.showCurrentInStatus": true,
  "python.analysis.autoSearchPaths": false,
  "python.analysis.enablePytestSupport": false,
  "python.analysis.exclude": [
    "**/.cache/**",
    "**/.pio/**",
    "**/.trunk/*actions",
    "**/.trunk/*logs",
    "**/.trunk/*notifications",
    "**/.trunk/*out",
    "**/.trunk/*plugins",
    "**/.vscode/**",
    "**/bower_components/**",
    "**/node_modules/**",
    "**/site-packages/**",
    "**/site/**",
  ],
  "python.analysis.typeCheckingMode": "basic",
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "redhat.telemetry.enabled": false,
  "remoteHub.richNavigation.enabled": true,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
  "scm.providerCountBadge": "visible",
  "scm.showIncomingChanges": "always",
  "scm.showOutgoingChanges": "always",
  "search.collapseResults": "auto",
  "search.defaultViewMode": "tree",
  "search.exclude": {
    "**/.cache/**": true,
    "**/.git/**": true,
    "**/.pio/**": true,
    "**/.trunk/*actions": true,
    "**/.trunk/*logs": true,
    "**/.trunk/*notifications": true,
    "**/.trunk/*out": true,
    "**/.trunk/*plugins": true,
    "**/bower_components/**": true,
    "**/node_modules/**": true,
    "**/site-packages/**": true,
    "**/site/**": true
  },
  "search.showLineNumbers": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableFileLinks": "off",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.lineHeight": 0,
  "terminal.integrated.tabs.hideCondition": "never",
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.tabs.showActions": "always",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "testing.gutterEnabled": true,
  "todo-tree.filtering.ignoreGitSubmodules": true,
  "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
  "trunk.inlineDecorators": false,
  "trunk.inlineDecoratorsForAllExtensions": true,
  "trunk.showWhatsNew": false,
  "trunk.showWhatsNewMessage": false,
  "trunk.trunkPath": ".vscode/.trunk/",
  "typescript.tsc.autoDetect": "off",
  "vscode-serial-monitor.dataBits": 8,
  "vscode-serial-monitor.lineEnding": "\n",
  "vscode-serial-monitor.parity": "none",
  "vscode-serial-monitor.stopBits": 1,
  "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "yaml.customTags": [
    "!ENV scalar",
    "!ENV sequence",
    "!relative scalar",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
    "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format"
  ],
  "yaml.extension.recommendations": false,
  "yaml.format.printWidth": 128,
  "yaml.format.proseWrap": "never",
  "yaml.schemas": {
    "https://github.com/SchemaStore/schemastore/raw/master/src/schemas/json/github-issue-config.json": ".github/ISSUE_TEMPLATE/config.yml",
    "https://github.com/SchemaStore/schemastore/raw/master/src/schemas/json/github-issue-forms.json": ".github/ISSUE_TEMPLATE/*.yml",
    "https://github.com/SchemaStore/schemastore/raw/master/src/schemas/json/github-workflow.json": ".github/workflows/*.yml",
    "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml"
  }
}
