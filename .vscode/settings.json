{
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "vscode.markdown-language-features",
    "editor.wordWrap": "off"
  },
  "[python]": {
    "editor.autoIndent": "full",
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "black-formatter.importStrategy": "fromEnvironment",
  "C_Cpp.autoAddFileAssociations": true,
  "C_Cpp.autocomplete": "default",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_path": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.codeAnalysis.clangTidy.codeAction.formatFixes": true,
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.path": "C:\\Program Files\\LLVM\\bin\\clang-tidy.exe",
  "C_Cpp.codeAnalysis.clangTidy.useBuildPath": false,
  "C_Cpp.codeFolding": "enabled",
  "C_Cpp.configurationWarnings": "enabled",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "C_Cpp.default.compileCommands": "compile_commands.json",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.mergeConfigurations": true,
  "C_Cpp.dimInactiveRegions": true,
  "C_Cpp.doxygen.generatedStyle": "/**",
  "C_Cpp.enhancedColorization": "enabled",
  "C_Cpp.files.exclude": { // see search.exclude
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "site": true
  },
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.hover": "default",
  "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
  "C_Cpp.inlayHints.parameterNames.hideLeadingUnderscores": false,
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "C_Cpp.vcFormat.indent.caseContentsWhenBlock": true,
  "C_Cpp.vcFormat.newLine.beforeCatch": true,
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": false,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": false,
  "C_Cpp.vcFormat.wrap.preserveBlocks": "allOneLineScopes",
  "clang-format.executable": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "cmake.allowCommentsInPresetsFile": true,
  "cmake.allowUnsupportedPresetsVersions": true,
  "cmake.ctest.allowParallelJobs": true,
  "cmake.ignoreCMakeListsMissing": true,
  "cmake.options.statusBarVisibility": "compact",
  "debug.console.wordWrap": false,
  "debug.showInStatusBar": "always",
  "debug.showSubSessionsInToolBar": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.wordWrap": "off",
  "doxdocgen.generic.commandSuggestionAddPrefix": true,
  "doxdocgen.generic.useGitUserEmail": true,
  "doxdocgen.generic.useGitUserName": true,
  "editor.detectIndentation": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.largeFileOptimizations": false,
  "editor.showFoldingControls": "always",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "editor.unusualLineTerminators": "auto",
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 256,
  "editor.wrappingIndent": "deepIndent",
  "explorer.autoRevealExclude": { // see:files.exclude
    ".cache": true,
    ".git": true,
    ".pio": true,
    ".trunk": true,
    "site": true
  },
  "files.associations": {
    "*.json": "jsonc",
    "*.svd": "xml"
  },
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": { // see:explorer.autoRevealExclude
    ".cache": true,
    ".git": true,
    ".pio": true,
    ".trunk": true,
    "site": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.trimTrailingWhitespaceInRegexAndStrings": false,
  "files.watcherExclude": { // see:search.exclude
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "site": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "git.repositoryScanIgnoredFolders": [
    ".cache",
    ".devcontainer",
    ".pio",
    ".private",
    ".trunk",
    "site"
  ],
  "git.supportCancellation": true,
  "git.terminalGitEditor": true,
  "git.timeline.showUncommitted": true,
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "hexeditor.columnWidth": 16,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "hexeditor.showDecodedText": true,
  "idf.enableCCache": true,
  "idf.enableIdfComponentManager": true,
  "idf.showOnboardingOnInit": false,
  "idf.telemetry": false,
  "jake.autoDetect": "off",
  "markdown-checkboxes.enable": true,
  "markdown-mermaid.darkModeTheme": "dark",
  "markdown-mermaid.lightModeTheme": "neutral",
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableLinkify": false,
  "markdown-preview-enhanced.frontMatterRenderingOption": "table",
  "markdown-preview-enhanced.hideDefaultVSCodeMarkdownPreviewButtons": false,
  "markdown-preview-enhanced.mathRenderingOption": "None",
  "markdown-preview-enhanced.mermaidTheme": "forest",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "markdown-preview-enhanced.useGitHubStylePipedLink": true,
  "markdown-preview-github-styles.colorTheme": "dark",
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "markdown.extension.bold.indicator": "**",
  "markdown.extension.math.enabled": false,
  "markdown.extension.print.absoluteImgPath": false,
  "markdown.extension.print.validateUrls": false,
  // "markdown.extension.syntax.plainTheme": true,
  "markdown.extension.tableFormatter.enabled": true,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.math.enabled": false,
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preferredMdPathExtensionStyle": "includeExtension",
  "markdown.preview.typographer": true,
  "markdown.suggest.paths.includeWorkspaceHeaderCompletions": "onSingleOrDoubleHash",
  "markdown.updateLinksOnFileMove.enabled": "prompt",
  "markdown.validate.enabled": true,
  "markdown.validate.fileLinks.markdownFragmentLinks": "warning",
  "markdown.validate.unusedLinkDefinitions.enabled": "ignore",
  "markdownlint.config": {
    "MD012": false,
    "MD013": false,
    "MD031": false,
    "MD041": false,
    "MD047": false
  },
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.autoDetect": "off",
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "platformio-ide.autoCloseSerialMonitor": false,
  "platformio-ide.disablePIOHomeStartup": true,
  "problems.showCurrentInStatus": true,
  "search.exclude": {
    ".cache": true,
    ".devcontainer": true,
    ".git": true,
    ".github": true,
    ".pio": true,
    ".private": true,
    ".trunk": true,
    ".vscode": true,
    "site": true
  },
  "trunk.inlineDecoratorsForAllExtensions": false,
  "trunk.showWhatsNew": false,
  "trunk.showWhatsNewMessage": false,
  "typescript.tsc.autoDetect": "off",
  "vscode-serial-monitor.lineEnding": "\n",
  "yaml.completion": true,
  "yaml.disableAdditionalProperties": false,
  "yaml.disableDefaultProperties": false,
  "yaml.format.proseWrap": "never",
  "yaml.validate": true,
  "yaml.schemas": {
    "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml"
  },
  "yaml.customTags": [
    "!ENV scalar",
    "!ENV sequence",
    "!relative scalar",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
    "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format"
  ]
}
